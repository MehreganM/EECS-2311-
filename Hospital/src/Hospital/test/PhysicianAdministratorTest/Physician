package Hospitalapping;
//implemented for testing
public class Physician {
    private String firstName;
    private String lastName;
    private int age;
    private String gender;
    private String address;
    private String specialty;
    private PhysicianAdministrator admin; 

 // implemented for testing
    public Physician(String firstName, String lastName, int age, String gender, String address, String specialty) {
        this.firstName = firstName;
        this.lastName = lastName;
        this.age = age;
        this.gender = gender;
        this.address = address;
        this.specialty = specialty;
    }

 // implemented for testing
    public void setAdministrator(PhysicianAdministrator admin) {
        this.admin = admin;
    }
 // implemented for testing
    public String getSpecialty() {
        return specialty;
    }
 // implemented for testing
    public String getName() {
        return firstName + " " + lastName;
    }

}
